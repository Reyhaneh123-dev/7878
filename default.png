  </span><span><span class="hljs-keyword">else</span></span><span>{box.</span><span><span class="hljs-property">innerText</span></span><span>=</span><span><span class="hljs-string">"نمایش اعضا برای این مجموعه امکان‌پذیر نیست."</span></span><span>;}
