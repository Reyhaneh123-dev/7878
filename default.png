</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">showVenn</span></span><span>(</span><span><span class="hljs-params"></span></span><span>){
  </span><span><span class="hljs-keyword">const</span></span><span> canvas = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"vennCanvas"</span></span><span>);
  </span><span><span class="hljs-keyword">const</span></span><span> ctx = canvas.</span><span><span class="hljs-title function_">getContext</span></span><span>(</span><span><span class="hljs-string">"2d"</span></span><span>);
  ctx.</span><span><span class="hljs-title function_">clearRect</span></span><span>(</span><span><span class="hljs-number">0</span></span><span>,</span><span><span class="hljs-number">0</span></span><span>,canvas.</span><span><span class="hljs-property">width</span></span><span>,canvas.</span><span><span class="hljs-property">height</span></span><span>);
  </span><span><span class="hljs-keyword">let</span></span><span> set = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"knownSet"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>;
  </span><span><span class="hljs-keyword">if</span></span><span>(set===</span><span><span class="hljs-string">"Q"</span></span><span> || set===</span><span><span class="hljs-string">"I"</span></span><span> || set===</span><span><span class="hljs-string">"R"</span></span><span>){ 
      ctx.</span><span><span class="hljs-property">font</span></span><span>=</span><span><span class="hljs-string">"18px Tahoma"</span></span><span>; ctx.</span><span><span class="hljs-property">fillStyle</span></span><span>=</span><span><span class="hljs-string">"red"</span></span><span>;
      ctx.</span><span><span class="hljs-title function_">fillText</span></span><span>(</span><span><span class="hljs-string">"نمایش نمودار امکان‌پذیر نیست"</span></span><span>, </span><span><span class="hljs-number">20</span></span><span>, </span><span><span class="hljs-number">50</span></span><span>); 
