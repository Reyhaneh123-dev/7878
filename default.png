</span><span><span class="hljs-tag">&lt;<span class="hljs-name">script</span></span></span><span>&gt;<span class="language-javascript">
</span></span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">handleDisplayChange</span></span><span>(</span><span><span class="hljs-params"></span></span><span>){
    </span><span><span class="hljs-keyword">const</span></span><span> displayType = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'displayType'</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>;
    </span><span><span class="hljs-keyword">const</span></span><span> knownSetSelect = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'knownSetSelect'</span></span><span>);
    </span><span><span class="hljs-keyword">const</span></span><span> memberBox = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'memberBox'</span></span><span>);

    </span><span><span class="hljs-comment">// مخفی یا نمایش dropdown دوم</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span>(displayType === </span><span><span class="hljs-string">'knownSet'</span></span><span>){
        knownSetSelect.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">remove</span></span><span>(</span><span><span class="hljs-string">'hidden'</span></span><span>);
        memberBox.</span><span><span class="hljs-property">innerText</span></span><span> = </span><span><span class="hljs-string">''</span></span><span>;
    } </span><span><span class="hljs-keyword">else</span></span><span> {
        knownSetSelect.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">add</span></span><span>(</span><span><span class="hljs-string">'hidden'</span></span><span>);
        memberBox.</span><span><span class="hljs-property">innerText</span></span><span> = </span><span><span class="hljs-string">''</span></span><span>;
    }
}

</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">showOperations</span></span><span>(</span><span><span class="hljs-params"></span></span><span>){
    </span><span><span class="hljs-keyword">const</span></span><span> displayType = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'displayType'</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>;
    </span><span><span class="hljs-keyword">const</span></span><span> knownSetSelect = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'knownSetSelect'</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>;
    </span><span><span class="hljs-keyword">const</span></span><span> operation = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'operationSelect'</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>;
    </span><span><span class="hljs-keyword">const</span></span><span> memberBox = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'memberBox'</span></span><span>);

    </span><span><span class="hljs-keyword">let</span></span><span> A_math = </span><span><span class="hljs-string">"{ x ∈ A | x ≥ 1 }"</span></span><span>;  </span><span><span class="hljs-comment">// مجموعه A برای مثال</span></span><span>
    </span><span><span class="hljs-keyword">let</span></span><span> knownSet_math = </span><span><span class="hljs-string">''</span></span><span>;

    </span><span><span class="hljs-keyword">if</span></span><span>(displayType === </span><span><span class="hljs-string">'math'</span></span><span>){
        memberBox.</span><span><span class="hljs-property">innerText</span></span><span> = A_math;
        </span><span><span class="hljs-keyword">return</span></span><span>;
    }
    </span><span><span class="hljs-keyword">if</span></span><span>(displayType === </span><span><span class="hljs-string">'list'</span></span><span>){
        memberBox.</span><span><span class="hljs-property">innerText</span></span><span> = </span><span><span class="hljs-string">"{1,2,3,...}"</span></span><span>;
        </span><span><span class="hljs-keyword">return</span></span><span>;
    }
    </span><span><span class="hljs-keyword">if</span></span><span>(displayType === </span><span><span class="hljs-string">'knownSet'</span></span><span>){
        </span><span><span class="hljs-keyword">if</span></span><span>(!knownSetSelect){
            </span><span><span class="hljs-title function_">alert</span></span><span>(</span><span><span class="hljs-string">"لطفاً یک مجموعه شناخته شده انتخاب کنید!"</span></span><span>);
            </span><span><span class="hljs-keyword">return</span></span><span>;
        }

        </span><span><span class="hljs-keyword">switch</span></span><span>(knownSetSelect){
            </span><span><span class="hljs-keyword">case</span></span><span> </span><span><span class="hljs-string">'N'</span></span><span>: knownSet_math = </span><span><span class="hljs-string">"{1,2,3,...}"</span></span><span>; </span><span><span class="hljs-keyword">break</span></span><span>;
            </span><span><span class="hljs-keyword">case</span></span><span> </span><span><span class="hljs-string">'W'</span></span><span>: knownSet_math = </span><span><span class="hljs-string">"{0,1,2,3,...}"</span></span><span>; </span><span><span class="hljs-keyword">break</span></span><span>;
            </span><span><span class="hljs-keyword">case</span></span><span> </span><span><span class="hljs-string">'Z'</span></span><span>: knownSet_math = </span><span><span class="hljs-string">"{..., -2,-1,0,1,2,...}"</span></span><span>; </span><span><span class="hljs-keyword">break</span></span><span>;
            </span><span><span class="hljs-keyword">case</span></span><span> </span><span><span class="hljs-string">'Q'</span></span><span>: knownSet_math = </span><span><span class="hljs-string">"{...} (اعداد گویا)"</span></span><span>; </span><span><span class="hljs-keyword">break</span></span><span>;
            </span><span><span class="hljs-keyword">case</span></span><span> </span><span><span class="hljs-string">'I'</span></span><span>: knownSet_math = </span><span><span class="hljs-string">"{...} (اعداد گنگ)"</span></span><span>; </span><span><span class="hljs-keyword">break</span></span><span>;
            </span><span><span class="hljs-keyword">case</span></span><span> </span><span><span class="hljs-string">'R'</span></span><span>: knownSet_math = </span><span><span class="hljs-string">"{...} (اعداد حقیقی)"</span></span><span>; </span><span><span class="hljs-keyword">break</span></span><span>;
        }

        </span><span><span class="hljs-keyword">let</span></span><span> result = </span><span><span class="hljs-string">''</span></span><span>;
        </span><span><span class="hljs-keyword">switch</span></span><span>(operation){
            </span><span><span class="hljs-keyword">case</span></span><span> </span><span><span class="hljs-string">'union'</span></span><span>: result = </span><span><span class="hljs-string">`A ∪ <span class="hljs-subst">${knownSet_math}</span></span></span><span>`; </span><span><span class="hljs-keyword">break</span></span><span>;
            </span><span><span class="hljs-keyword">case</span></span><span> </span><span><span class="hljs-string">'intersection'</span></span><span>: result = </span><span><span class="hljs-string">`A ∩ <span class="hljs-subst">${knownSet_math}</span></span></span><span>`; </span><span><span class="hljs-keyword">break</span></span><span>;
            </span><span><span class="hljs-keyword">case</span></span><span> </span><span><span class="hljs-string">'difference'</span></span><span>: result = </span><span><span class="hljs-string">`A \\ <span class="hljs-subst">${knownSet_math}</span></span></span><span>`; </span><span><span class="hljs-keyword">break</span></span><span>;
            </span><span><span class="hljs-keyword">case</span></span><span> </span><span><span class="hljs-string">'subset'</span></span><span>: result = </span><span><span class="hljs-string">`A ⊆ <span class="hljs-subst">${knownSet_math}</span></span></span><span> ?`; </span><span><span class="hljs-keyword">break</span></span><span>;
        }
        memberBox.</span><span><span class="hljs-property">innerText</span></span><span> = result;
    }
}
</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">script</span></span></span><span>&gt;

