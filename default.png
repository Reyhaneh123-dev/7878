</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">makeExpression</span></span><span>(</span><span><span class="hljs-params"></span></span><span>){
  </span><span><span class="hljs-keyword">let</span></span><span> set = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"knownSet"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>;
  </span><span><span class="hljs-keyword">let</span></span><span> cond = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"textCondition"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>.</span><span><span class="hljs-title function_">trim</span></span><span>();
