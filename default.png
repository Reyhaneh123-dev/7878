  }
  </span><span><span class="hljs-keyword">let</span></span><span> cx=canvas.</span><span><span class="hljs-property">width</span></span><span>/</span><span><span class="hljs-number">2</span></span><span>, cy=canvas.</span><span><span class="hljs-property">height</span></span><span>/</span><span><span class="hljs-number">2</span></span><span>, r=</span><span><span class="hljs-number">50</span></span><span>;
  </span><span><span class="hljs-keyword">let</span></span><span> angle=</span><span><span class="hljs-number">0</span></span><span>;
  </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">animate</span></span><span>(</span><span><span class="hljs-params"></span></span><span>){
    ctx.</span><span><span class="hljs-title function_">clearRect</span></span><span>(</span><span><span class="hljs-number">0</span></span><span>,</span><span><span class="hljs-number">0</span></span><span>,canvas.</span><span><span class="hljs-property">width</span></span><span>,canvas.</span><span><span class="hljs-property">height</span></span><span>);
    ctx.</span><span><span class="hljs-title function_">beginPath</span></span><span>();
    ctx.</span><span><span class="hljs-title function_">arc</span></span><span>(cx,cy,r*angle/</span><span><span class="hljs-number">100</span></span><span>,</span><span><span class="hljs-number">0</span></span><span>,</span><span><span class="hljs-number">2</span></span><span>*</span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-property">PI</span></span><span>*angle/</span><span><span class="hljs-number">100</span></span><span>);
