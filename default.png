&lt;!DOCTYPE html&gt;
&lt;html lang="fa" dir="rtl"&gt;
&lt;head&gt;
&lt;meta charset="UTF-8"&gt;
&lt;meta name="viewport" content="width=device-width, initial-scale=1.0"&gt;
&lt;title&gt;پروفایل کاربری&lt;/title&gt;
&lt;style&gt;
body { font-family: Tahoma,sans-serif; background:#f1f8e9; color:#222; padding:20px; text-align:center;}
label, input, button { display:block; margin:15px auto; font-size:18px; }
img { width:100px; height:100px; border-radius:50%; margin-top:20px; cursor:pointer; }
button { padding:10px 20px; border:none; background:#1976d2; color:white; border-radius:8px; cursor:pointer; }
button:hover { background:#0d47a1; }
&lt;/style&gt;
&lt;/head&gt;
&lt;body&gt;

&lt;h2&gt;ثبت اطلاعات کاربری&lt;/h2&gt;

&lt;img id="profileImg" src="default.png" alt="پروفایل"&gt;

&lt;label&gt;نام:&lt;/label&gt;
&lt;input type="text" id="firstName" placeholder="نام خود را وارد کنید"&gt;

&lt;label&gt;نام خانوادگی:&lt;/label&gt;
&lt;input type="text" id="lastName" placeholder="نام خانوادگی خود را وارد کنید"&gt;

&lt;button onclick="saveProfile()"&gt;ذخیره و ادامه&lt;/button&gt;

&lt;script&gt;
function saveProfile(){
    let first = document.getElementById('firstName').value.trim();
    let last = document.getElementById('lastName').value.trim();
    if(!first || !last){ alert('نام و نام خانوادگی را وارد کنید'); return; }
    localStorage.setItem('firstName', first);
    localStorage.setItem('lastName', last);
    window.location.href = 'page3.html';
}
&lt;/script&gt;

