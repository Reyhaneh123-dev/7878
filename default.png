  </span><span><span class="hljs-keyword">let</span></span><span> startX=</span><span><span class="hljs-number">50</span></span><span>,endX=canvas.</span><span><span class="hljs-property">width</span></span><span>-</span><span><span class="hljs-number">50</span></span><span>,y=</span><span><span class="hljs-number">100</span></span><span>;
  ctx.</span><span><span class="hljs-title function_">beginPath</span></span><span>();
