  </span><span><span class="hljs-keyword">let</span></span><span> expression = </span><span><span class="hljs-string">`{ x ∈ <span class="hljs-subst">${set}</span></span></span><span> | </span><span><span class="hljs-subst">${convertCondition(cond)}</span></span><span> }`;
  </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"mathResult"</span></span><span>).</span><span><span class="hljs-property">innerText</span></span><span> = expression;
}

</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">showMembers</span></span><span>(</span><span><span class="hljs-params"></span></span><span>){
